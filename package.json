{
    "homepage": "http://theahmadchand.github.io/potfolio-react-ts",
    "name": "react-potfolio",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "build": "webpack",
        "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
        "dev": "webpack-dev-server --env mode=development",
        "prod": "npm run build -- --env mode=production",
        "prod:typescript": "npm run prod -- --env presets=typescript",
        "prod:analyze": "npm run prod -- --env presets=analyze",
        "prod:compress": "npm run prod -- --env presets=compress",
        "dev:debug": "npm run debug -- --env mode=development",
        "prod:debug": "npm run debug -- --env mode=production",
        "predeploy": "npm run prod",
        "deploy": "gh-pages -d -- npm run prod"
    },
    "devDependencies": {
        "@babel/core": "^7.22.1",
        "@babel/preset-env": "^7.22.4",
        "@babel/preset-react": "^7.22.3",
        "@babel/preset-typescript": "^7.21.5",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.2.5",
        "@types/react": "^18.2.8",
        "@types/react-dom": "^18.2.4",
        "babel-loader": "^9.1.2",
        "css-loader": "^6.8.1",
        "file-loader": "^6.2.0",
        "gh-pages": "^5.0.0",
        "html-webpack-plugin": "^5.5.1",
        "mini-css-extract-plugin": "^2.7.6",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sass": "^1.62.1",
        "sass-loader": "^13.3.1",
        "style-loader": "^3.3.3",
        "ts-loader": "^9.4.3",
        "typescript": "^5.1.3",
        "url-loader": "^4.1.1",
        "webpack": "^5.85.0",
        "webpack-bundle-analyzer": "^4.9.0",
        "webpack-cli": "^5.1.2",
        "webpack-dev-server": "^4.15.0",
        "webpack-merge": "^5.9.0"
    }
}
